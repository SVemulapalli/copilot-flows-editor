{
  "name": "@openchatai/copilot-flows-editor",
  "version": "1.3.3",
  "private": false,
  "description": "OpenChat Flow Builder for openCopilot",
  "type": "module",
  "main": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.es.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test:watch": "vitest",
    "test": "vitest --run",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build -c vite.config.lib.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run test && npm run build:lib",
    "semantic-release": "semantic-release",
    "cs": "changeset",
    "cs:v": "changeset version"
  },
  "peerDependencies": {
    "react": "^18.x",
    "react-dom": "^18.x"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "^6.9.0",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lint": "^6.4.1",
    "@codemirror/view": "^6.17.1",
    "@fontsource-variable/open-sans": "^5.0.13",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@sentry/react": "^7.69.0",
    "@sentry/vite-plugin": "^2.7.1",
    "@types/js-beautify": "^1.14.0",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@uiw/codemirror-extensions-basic-setup": "^4.21.13",
    "@uiw/codemirror-themes-all": "^4.21.13",
    "@uiw/react-codemirror": "^4.21.13",
    "@vitejs/plugin-react": "^4.0.4",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "immer": "^10.0.2",
    "js-beautify": "^1.14.9",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.8.3",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.4",
    "vitest": "^0.34.5",
    "@changesets/cli": "^2.26.2"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ah7255703/schema-parser-ui.git"
  }
}